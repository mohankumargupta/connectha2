{
  "expo": {
    "name": "BigButton:Home Assistant",
    "slug": "BigButtonHA",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon1024x1024.png",
    "scheme": "bigbuttonha",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon1024x1024.png",
        "backgroundColor": "#007260"
      },
      "package": "com.mohankumargupta.connectha2"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      "expo-secure-store",
      ["expo-font",
       {
          "fonts": ["node_modules/@expo-google-fonts/work-sans/WorkSans_400Regular.ttf"]
       }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "900a7575-8d21-4be4-9e95-fd96f4931d6a"
      }
    },
    "owner": "raspberrypisig"
  }
}
